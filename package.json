{
  "name": "n8n-templates-collection",
  "version": "1.0.0",
  "description": "A comprehensive collection of production-ready n8n workflow templates and automation blueprints",
  "main": "index.js",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sachin-deshik-10/N8N_TEMPLATES.git"
  },
  "keywords": [
    "n8n",
    "workflow",
    "automation",
    "templates",
    "no-code",
    "low-code",
    "integration",
    "api",
    "webhooks",
    "ai",
    "langchain",
    "workflows"
  ],
  "author": {
    "name": "Sachin Deshik",
    "url": "https://github.com/sachin-deshik-10"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sachin-deshik-10/N8N_TEMPLATES/issues"
  },
  "homepage": "https://github.com/sachin-deshik-10/N8N_TEMPLATES#readme",
  "engines": {
    "node": ">=18.17 <=22",
    "pnpm": ">=10.2.1"
  },
  "packageManager": "pnpm@10.2.1",
  "scripts": {
    "prepare": "husky install",
    "dev": "n8n",
    "start": "n8n start",
    "build": "echo 'Building templates...' && node scripts/build-templates.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .js,.ts,.json --fix",
    "lint:check": "eslint . --ext .js,.ts,.json",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "validate": "npm run lint:check && npm run format:check && npm run test",
    "validate-templates": "node scripts/validate-templates.js",
    "generate-docs": "node scripts/generate-docs.js",
    "clean": "rimraf dist coverage .nyc_output",
    "precommit": "lint-staged",
    "prepare-release": "npm run validate && npm run build"
  },
  "dependencies": {
    "n8n": "^1.83.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.17",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "lint-staged": {
    "*.{js,ts,json}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,yml,yaml}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "scripts/**/*.{js,ts}",
      "!scripts/**/*.test.{js,ts}",
      "!**/node_modules/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.(js|ts)",
      "**/*.(test|spec).(js|ts)"
    ]
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended",
      "prettier"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint",
      "jest",
      "json"
    ],
    "env": {
      "node": true,
      "jest": true,
      "es6": true
    },
    "rules": {
      "no-console": "warn",
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/no-explicit-any": "warn"
    }
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/sachin-deshik-10"
  }
}
    "@types/jest": "^29.5.3",
    "@types/node": "*",
    "@types/supertest": "^6.0.2",
    "cross-env": "^7.0.3",
    "jest": "^29.6.2",
    "jest-environment-jsdom": "^29.6.2",
    "jest-expect-message": "^1.1.3",
    "jest-mock": "^29.6.2",
    "jest-mock-extended": "^3.0.4",
    "lefthook": "^1.7.15",
    "nock": "^14.0.1",
    "nodemon": "^3.0.1",
    "npm-run-all2": "^7.0.2",
    "p-limit": "^3.1.0",
    "rimraf": "^5.0.1",
    "run-script-os": "^1.0.7",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.10",
    "tsc-watch": "^6.2.0",
    "turbo": "2.3.3",
    "typescript": "*",
    "zx": "^8.1.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sqlite3"
    ],
    "overrides": {
      "@types/node": "^18.16.16",
      "chokidar": "^4.0.1",
      "esbuild": "^0.24.0",
      "formidable": "3.5.1",
      "pug": "^3.0.3",
      "semver": "^7.5.4",
      "tslib": "^2.6.2",
      "tsconfig-paths": "^4.2.0",
      "typescript": "^5.8.2",
      "vue-tsc": "^2.2.8",
      "ws": ">=8.17.1",
      "@rudderstack/rudder-sdk-node>axios": "1.8.2",
      "ibm-cloud-sdk-core>axios": "1.8.2",
      "langchain>axios": "1.8.2",
      "snowflake-sdk>axios": "1.8.2"
    },
    "patchedDependencies": {
      "bull@4.12.1": "patches/bull@4.12.1.patch",
      "pkce-challenge@3.0.0": "patches/pkce-challenge@3.0.0.patch",
      "pyodide@0.23.4": "patches/pyodide@0.23.4.patch",
      "@types/express-serve-static-core@4.17.43": "patches/@types__express-serve-static-core@4.17.43.patch",
      "@types/ws@8.5.4": "patches/@types__ws@8.5.4.patch",
      "@types/uuencode@0.0.3": "patches/@types__uuencode@0.0.3.patch",
      "vue-tsc@2.2.8": "patches/vue-tsc@2.2.8.patch",
      "eslint-plugin-n8n-local-rules": "patches/eslint-plugin-n8n-local-rules.patch"
    }
  }
}
